footer.foot
	.section.foot-cont
		a(href='#').foot-title
			span Leka.kz
			span since 1989
		.foot-col.foot-nav-col
			button(data-click-action='front:asideShow').foot-col-button
				span.foot-subtitle Навигация
				span
			nav.foot-nav.foot-navigation
				a(href='#').foot-nav-link Каталог
				a(href='#').foot-nav-link Новости и акции
				a(href='#').foot-nav-link Услуги
				a(href='#').foot-nav-link Контакты
				a(href='#').foot-nav-link О компании
		.foot-col.foot-prod-col
			button(data-click-action='front:asideShow').foot-col-button
				span.foot-subtitle Продукты
				span
			nav.foot-nav.foot-products
				a(href='#').foot-nav-link Вентиляция
				a(href='#').foot-nav-link Лазерная резка
				a(href='#').foot-nav-link Противопожарное оборудование
				a(href='#').foot-nav-link Металлоизделия
				a(href='#').foot-nav-link Эксклюзив
		.foot-form
			h5.foot-form-subtitle Подпишитесь на рассылку новостей и акций
			input(type='text' placeholder='E-mail').foot-form-input
			button.button-arrow.foot-form-button
				svg(width="53" height="19" viewBox="0 0 53 19" fill="none" xmlns="http://www.w3.org/2000/svg")
					path(d="M0.178864 9.35676H51.5M51.5 9.35676L42.0569 0.5448M51.5 9.35676L42.0569 18.1687")
	.foot-priv
		.section
			span.priv-first Официальный сайт
				strong ТОО "Лека"
				|© 1989 - 2020
			a(href='#').priv-link Privacy Policy
			.priv-dev
				span Website Development:
				a(href='https://grammla.kz')
					+img('grammla.svg')
